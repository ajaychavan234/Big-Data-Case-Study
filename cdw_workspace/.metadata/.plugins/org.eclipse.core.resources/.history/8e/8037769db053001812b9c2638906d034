package com.cdw.dao;

import java.sql.SQLException;

import com.cdw.model.Transaction;
import com.cdw.resources.Queries;

public class TransactionDAO extends AbstractDAO{
	
	public Transaction getTranByZip (String zip, int month, int year) {
		Transaction  transaction = null;
		establishConnection();
		String query = Queries.Get_Tran_By_ZIP;

		try {
			state = conn.prepareStatement(query);
			state.setString(1, zip);
			state.setInt(1, month);
			state.setInt(1, year);
			result = state.executeQuery();
			
			if (result.next()) {
				transaction = new Transaction();
				String creditNo = result.getString(1);
			}
		} catch (SQLException e) {
			e.printStackTrace();
		}
		
		
	}


}
